cumentos/React/shop/src/store/shop/actions.ts",[],"/home/adernio/Documentos/React/shop/src/store/shop/reducer.ts",[],"/home/adernio/Documentos/React/shop/src/components/Quantity/styles.ts",[],"/home/adernio/Documentos/React/shop/src/components/Quantity/index.tsx",["169"],"import React, { useState } from 'react';\nimport { useDispatch } from 'react-redux';\nimport { useTheme } from 'styled-components';\nimport { TableRow, Row, Number, Select } from './styles';\nimport { removeItemCart, updateTotalValue } from '../../store/shop/actions';\nimport Title from '../Title';\nimport { IoMdTennisball } from 'react-icons/io';\n\ninterface IQuantity {\n    item: ProductCart;\n}\n\nconst Quantity:React.FC<IQuantity> = ({ item }) => {\n    const theme = useTheme();\n    const dispatch = useDispatch();\n    const [ total, setTotal ] = useState(item.counter)\n\n    const action = (type:string) => {\n        if(type === 'add'){ \n            setTotal(total + 1)\n            dispatch(updateTotalValue({type:'add',price:item.price, counter:total+1, id: item.id}))\n        } else{ \n            const decrement = total - 1\n            if(decrement === 0){\n                dispatch(removeItemCart({id:item.id, price:item.price}))\n            }\n            setTotal(decrement) \n            dispatch(updateTotalValue({type:'remove',price:item.price, counter:total-1, id: item.id }))\n        }\n    }\n\n    return(\n        <TableRow>\n            <Row>\n                <Select color={theme.colors.danger} onClick={()=>action('remove')}>-</Select>\n                <Number>{ total }</Number>\n                <Select color={theme.colors.success} onClick={()=>action('add')}>+</Select>\n            </Row>\n            <Title size={22}>{item.name}</Title>\n            <Title size={22}>{`R$ ${(total * item.price).toFixed(2)}`}</Title>\n        </TableRow>\n    )\n}\n\nexport default Quantity","/home/adernio/Documentos/React/shop/src/store/index.ts",[],"/home/adernio/Documentos/React/shop/src/compo